<%- include('./includes/head.ejs') %>
  <body>
    <%- include('./includes/navigation.ejs') %>
    <div class="container">
      <main class="results">
        <ul class="nav nav-pills nav-fill my-3" id="pills-tab" role="tablist">
          <li class="nav-item">
            <a
              class="nav-link active"
              id="pills-visualization-tab"
              data-toggle="pill"
              href="#pills-visualization"
              role="tab"
              aria-controls="pills-visualization"
              aria-selected="true"
              >Visualizations</a
            >
          </li>
          <li class="nav-item">
            <a
              class="nav-link"
              id="pills-algorithm-tab"
              data-toggle="pill"
              href="#pills-algorithm"
              role="tab"
              aria-controls="pills-algorithm"
              aria-selected="false"
              >Predictions</a
            >
          </li>
          <li class="nav-item">
            <a
              class="nav-link"
              id="pills-scores-tab"
              data-toggle="pill"
              href="#pills-scores"
              role="tab"
              aria-controls="pills-scores"
              aria-selected="false"
              >Scores</a
            >
          </li>
        </ul>
        <div class="tab-content" id="pills-tabContent">
          <div
            class="tab-pane fade show active"
            id="pills-visualization"
            role="tabpanel"
            aria-labelledby="pills-visualization-tab"
          >
            <h3>All Visualizations</h3>
              <% for (let i = 0; i < images.length - 1;  i++ ) { %>
                <div class="tray">
                  <h5 class="title"><%=imgTitles[i]%></h5>
                  <img src="<%=images[i]%>" alt="<%=imgTitles[i]%>"/>
                </div>
              <% };%>
          </div>
          <div
            class="tab-pane fade"
            id="pills-algorithm"
            role="tabpanel"
            aria-labelledby="pills-algorithm-tab"
          >
            <h3>Prediction Figures</h3>
              <% for (let i = 0; i < algorithms.length -1;  i++ ) { %>
                <div class="tray">
                  <h5 class="title"><%=algoTitles[i]%></h5>
                  <img src="<%=algorithms[i]%>" alt="<%=algoTitles[i]%>"/>
                </div>
              <% };%>
          </div>
          <div
            class="tab-pane fade"
            id="pills-scores"
            role="tabpanel"
            aria-labelledby="pills-scores-tab"
          >
          <% for (let i = 0; i < accuracyScores.length - 1;  i++ ) { %>
            <div class="scores">
              <h5 class="title"><%=accuracyScores[i][0]%>: <span><%=accuracyScores[i][1]%></span></h5>
            </div>
          <% };%>
          <div class="mt-5 text-center text-capitalize">
            <p>
              Upload another dataset? <a type="button" href="/">Home</a>
            </p>
          </div>
        </div>
        </div>
      </main>
    </div>
    <%- include('./includes/footer.ejs') %>
  </body>
</html>
